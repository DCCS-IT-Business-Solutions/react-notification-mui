  "repository": {
    "type": "git",
    "url": "git+https://github.com/DCCS-IT-Business-Solutions/react-notification-mui.git"
  },
  "keywords": [
    "npm",
    "node",
    "react",
    "javascript",
    "typescript",
    "notification",
    "snackbar",
    "react-notification",
    "react-snackbar",
    "mui",
    "material-ui",
    "easy to use"
  ],
  "author": "Andreas Resch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DCCS-IT-Business-Solutions/react-notification-mui/issues"
  },
  "homepage": "https://github.com/DCCS-IT-Business-Solutions/react-notification-mui#readme",
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "@material-ui/core": "^4.3.1",
    "@material-ui/icons": "^4.2.1",
    "@types/moment": "^2.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@material-ui/core": "4.3.1",
    "@material-ui/icons": "4.2.1",
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-links": "^5.1.10",
    "@storybook/addons": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "@types/jest": "^24.0.17",
    "@types/moment": "^2.13.0",
    "@types/react": "16.8.25",
    "@types/react-dom": "16.8.5",
    "@types/react-test-renderer": "^16.9.2",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.18",
    "@testing-library/react": "^10.0.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^10.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "ts-jest": "^25.2.1",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3",
    "prettier": "^2.0.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "c**/?(*.)(spec|test).ts?(x)"
